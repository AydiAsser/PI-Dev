{% extends 'base.html.twig' %}

{% block title %}Prescriptions{% endblock %}

{% block body %}

 <div class="bradcam_area breadcam_bg">
        <div class="container">
            <div class="row">
                <div class="col-xl-12">
                    <div class="bradcam_text">
                        <h3>prescription</h3>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <h1>Prescriptions</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ prescription.id }}</td>
            </tr>
            <tr>
                <th>Frequance</th>
                <td>{{ prescription.frequance }}</td>
            </tr>
            <tr>
                <th>Date_debut</th>
                <td>{{ prescription.dateDebut ? prescription.dateDebut|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Date_fin</th>
                <td>{{ prescription.dateFin ? prescription.dateFin|date('Y-m-d') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_prescriptions_index') }}">back to list</a>

    <a href="{{ path('app_prescriptions_edit', {'id': prescription.id}) }}">edit</a>

    {{ include('prescriptions/_delete_form.html.twig') }}
{% endblock %}
